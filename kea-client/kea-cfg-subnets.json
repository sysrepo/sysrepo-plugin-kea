{
  "Dhcp6": {
    "control-socket": {
      "socket-type": "unix",
      "socket-name": "/tmp/kea-control-channel"
    },
    "interfaces-config": {
      "interfaces": [
        "eth1"
      ]
    },
    "lease-database": {
      "type": "memfile"
    },
    "preferred-lifetime": 3000,
    "valid-lifetime": 4000,
    "renew-timer": 1000,
    "rebind-timer": 2000,
    "subnet6": [
      {
        "subnet": "2001:db8::/32",
        "pools": [
          {
            "pool": "2001:db8:bad:c0de::/64"
          }
        ]
      },
      {
        "subnet": "2001:db9::/32",
        "pools": [
          {
            "pool": "2001:db9:bad:c0de::/64"
          }
        ]
      },
      {
        "subnet": "2001:dba::/32",
        "pools": [
          {
            "pool": "2001:dba:bad:c0de::/64"
          }
        ]
      },
      {
        "subnet": "2001:dbb::/32",
        "pools": [
          {
            "pool": "2001:dbb:bad:c0de::/64"
          }
        ]
      }
    ]
  },
  "Logging": {
    "loggers": [
      {
        "name": "kea-dhcp6",
        "output_options": [
          {
            "output": "stdout"
          }
        ],
        "debuglevel": 0,
        "severity": "DEBUG"
      }
    ]
  }
}
